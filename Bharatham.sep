    <subtitle duration="2669" effect="" end="4324872" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="983" start="4322203" style="Default" text="Stop this. A singer who vows never to sing." translation=""/>
    <subtitle duration="1699" effect="" end="4326671" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="984" start="4324972" style="Default" text="Isn't death better than that?" translation=""/>
    <subtitle duration="1000" effect="" end="4327771" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="985" start="4326771" style="Default" text="Never mind, Uncle" translation=""/>
    <subtitle duration="1000" effect="" end="4328871" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="986" start="4327871" style="Default" text="Brother is there" translation=""/>
    <subtitle duration="1366" effect="" end="4331549" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="987" start="4330183" style="Default" text="I will wash my face and legs" translation=""/>
    <subtitle duration="2473" effect="" end="4343624" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="988" start="4341151" style="Default" text="Appu is worried about you" translation=""/>
    <subtitle duration="2837" effect="" end="4356028" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="989" start="4353191" style="Default" text="Rama, Gopi will end it with the &lt;i&gt;Mangalam&lt;/i&gt;" translation=""/>
    <subtitle duration="1000" effect="" end="4357128" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="990" start="4356128" style="Default" text="He will never sing again, apparently" translation=""/>
    <subtitle duration="1955" effect="" end="4360198" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="991" start="4358243" style="Default" text="I'm the one who should end it all with the &lt;i&gt;Mangalam&lt;/i&gt;" translation=""/>
    <subtitle duration="1419" effect="" end="4361997" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="992" start="4360578" style="Default" text="Why does he need to do it?" translation=""/>
