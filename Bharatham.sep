    <subtitle duration="1000" effect="" end="5098920" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1067" start="5097920" style="Default" text="Will gold wither?" translation=""/>
    <subtitle duration="1456" effect="" end="5100476" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1068" start="5099020" style="Default" text="Have you even bought that?" translation=""/>
    <subtitle duration="2004" effect="" end="5103204" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1069" start="5101200" style="Default" text="We'll buy it tomorrow, Grandfather. Let's just wait for Brother" translation=""/>
    <subtitle duration="3858" effect="" end="5107162" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1070" start="5103304" style="Default" text="Kutta, this asfoetida which you've set aside might be a bit too much" translation=""/>
    <subtitle duration="3504" effect="" end="5110766" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1071" start="5107262" style="Default" text="Grandfather asked me to keep it there.&#10;Regardless of who told you to do it, it's still your responsibility" translation=""/>
    <subtitle duration="1000" effect="" end="5111866" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1072" start="5110866" style="Default" text="Gopi ..." translation=""/>
    <subtitle duration="1263" effect="" end="5113697" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1073" start="5112434" style="Default" text="Raman still isn't back" translation=""/>
    <subtitle duration="2022" effect="" end="5115819" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1074" start="5113797" style="Default" text="He'll be back; he left to get something done, didn't he?" translation=""/>
    <subtitle duration="1634" effect="" end="5117553" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1075" start="5115919" style="Default" text="Ramani is really worried" translation=""/>
    <subtitle duration="1381" effect="" end="5132139" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1076" start="5130758" style="Default" text="Sister-in-law, why are you so nervous?" translation=""/>
    <subtitle duration="1000" effect="" end="5134143" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1077" start="5133143" style="Default" text="Brother will be here soon" translation=""/>
    <subtitle duration="1000" effect="" end="5136534" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1078" start="5135534" style="Default" text="I'm afraid" translation=""/>
    <subtitle duration="2208" effect="" end="5138842" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1079" start="5136634" style="Default" text="that he's drunkenly fallen somewhere" translation=""/>
    <subtitle duration="2089" effect="" end="5141031" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1080" start="5138942" style="Default" text="Now, it's even worse than before" translation=""/>
    <subtitle duration="1212" effect="" end="5145713" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1081" start="5144501" style="Default" text="At the concert" translation=""/>
    <subtitle duration="1583" effect="" end="5148172" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1082" start="5146589" style="Default" text="He was ... quite drunk" translation=""/>
    <subtitle duration="1332" effect="" end="5149604" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1083" start="5148272" style="Default" text="He has no control" translation=""/>
    <subtitle duration="1954" effect="" end="5152517" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1084" start="5150563" style="Default" text="He has bought a closet full of bottles" translation=""/>
    <subtitle duration="1264" effect="" end="5154286" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1085" start="5153022" style="Default" text="He always wants to drink" translation=""/>
    <subtitle duration="1196" effect="" end="5155970" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1086" start="5154774" style="Default" text="And I offer no resistance" translation=""/>
    <subtitle duration="2458" effect="" end="5159001" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1087" start="5156543" style="Default" text="He asked, how does it matter now, if he drinks or doesn't drink" translation=""/>
    <subtitle duration="1000" effect="" end="5162892" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1088" start="5161892" style="Default" text="Gopi ..." translation=""/>
