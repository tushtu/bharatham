    <subtitle duration="1765" effect="" end="4372481" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="994" start="4370716" style="Default" text="Tomorrow, everyone will speak ill of you" translation=""/>
    <subtitle duration="2097" effect="" end="4374678" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="995" start="4372581" style="Default" text="Your father's soul will not forgive you, Rama" translation=""/>
    <subtitle duration="1000" effect="" end="4381698" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="996" start="4380698" style="Default" text="Rametta ..." translation=""/>
    <subtitle duration="1436" effect="" end="4384037" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="997" start="4382601" style="Default" text="Appu is our second son" translation=""/>
    <subtitle duration="2374" effect="" end="4386511" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="998" start="4384137" style="Default" text="You always said that our eldest son was Gopi" translation=""/>
    <subtitle duration="2058" effect="" end="4389728" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="999" start="4387670" style="Default" text="He has only sung what you taught him" translation=""/>
    <subtitle duration="2370" effect="" end="4392860" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1000" start="4390490" style="Default" text="He has only sat at your feet" translation=""/>
    <subtitle duration="5415" effect="" end="4398949" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1001" start="4393534" style="Default" text="Shouldn't we be happy to see him surpass even you and Father?" translation=""/>
    <subtitle duration="1000" effect="" end="4400049" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1002" start="4399049" style="Default" text="Yes" translation=""/>
    <subtitle duration="2076" effect="" end="4450070" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1003" start="4447994" style="Default" text="To end this sacrifice," translation=""/>
    <subtitle duration="1699" effect="" end="4451869" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1004" start="4450170" style="Default" text="You do not need your brother's permission" translation=""/>
    <subtitle duration="1474" effect="" end="4453443" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1005" start="4451969" style="Default" text="Nor your Guru's permission." translation=""/>
    <subtitle duration="1000" effect="" end="4454543" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1006" start="4453543" style="Default" text="Is that the case?" translation=""/>
    <subtitle duration="1592" effect="" end="4463996" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1007" start="4462404" style="Default" text="If you do not despise me" translation=""/>
    <subtitle duration="1000" effect="" end="4465096" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1008" start="4464096" style="Default" text="you must obey me." translation=""/>
    <subtitle duration="1090" effect="" end="4467197" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1009" start="4466107" style="Default" text="You must sing" translation=""/>
    <subtitle duration="2422" effect="" end="4470103" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1010" start="4467681" style="Default" text="Surpassing me and Father" translation=""/>
    <subtitle duration="1578" effect="" end="4471781" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1011" start="4470203" style="Default" text="should be your goal" translation=""/>
    <subtitle duration="1090" effect="" end="4474272" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1012" start="4473182" style="Default" text="You may offer the &lt;i&gt;dakshina&lt;/i&gt;" translation=""/>
    <subtitle duration="1000" effect="" end="4480583" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1013" start="4479583" style="Default" text="The &lt;i&gt;dakshina&lt;/i." translation=""/>
    <subtitle duration="1000" effect="" end="4481683" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1014" start="4480683" style="Default" text="is myself" translation=""/>
    <subtitle duration="1056" effect="" end="5005358" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1015" start="5004302" style="Default" text="There's a lot of &lt;i&gt;kasava&lt;/i&gt;" translation=""/>
    <subtitle duration="1000" effect="" end="5006458" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1016" start="5005458" style="Default" text="There isn't any on this side" translation=""/>
    <subtitle duration="1297" effect="" end="5009215" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1017" start="5007918" style="Default" text="These are nice saris, right Mother?" translation=""/>
    <subtitle duration="1000" effect="" end="5010315" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1018" start="5009315" style="Default" text="This one is really good." translation=""/>
    <subtitle duration="1000" effect="" end="5011415" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1019" start="5010415" style="Default" text="This is too much!" translation=""/>
    <subtitle duration="1000" effect="" end="5012515" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1020" start="5011515" style="Default" text="Let it be." translation=""/>
    <subtitle duration="1950" effect="" end="5014565" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1021" start="5012615" style="Default" text="All of them have an old person's taste" translation=""/>
    <subtitle duration="1499" effect="" end="5016620" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1022" start="5015121" style="Default" text="None of us selected them" translation=""/>
    <subtitle duration="1000" effect="" end="5017720" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1023" start="5016720" style="Default" text="It was a young person" translation=""/>
    <subtitle duration="1579" effect="" end="5019399" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1024" start="5017820" style="Default" text="your Gopi&#10;Ah, say that!" translation=""/>
    <subtitle duration="1000" effect="" end="5020499" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1025" start="5019499" style="Default" text="Then, how else can he improve?" translation=""/>
    <subtitle duration="1343" effect="" end="5021942" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1026" start="5020599" style="Default" text="I told him that I'd come with him" translation=""/>
    <subtitle duration="1000" effect="" end="5023042" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1027" start="5022042" style="Default" text="If only he listened. That Gopi!" translation=""/>
    <subtitle duration="1532" effect="" end="5025697" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1028" start="5024165" style="Default" text="Grandfather's taste is more tolerable than this" translation=""/>
    <subtitle duration="1000" effect="" end="5026797" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1029" start="5025797" style="Default" text="I think he has arrived" translation=""/>
    <subtitle duration="1765" effect="" end="5028662" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1030" start="5026897" style="Default" text="I'll teach him a lesson" translation=""/>
    <subtitle duration="1584" effect="" end="5030346" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1031" start="5028762" style="Default" text="What's the problem with this one?" translation=""/>
    <subtitle duration="1584" effect="" end="5032030" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1032" start="5030446" style="Default" text="I'm just messing with him, Mother" translation=""/>
    <subtitle duration="2645" effect="" end="5034775" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1033" start="5032130" style="Default" text="If Gopi gets angry, let him get angry" translation=""/>
    <subtitle duration="1045" effect="" end="5038498" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1034" start="5037453" style="Default" text="How was the concert?" translation=""/>
    <subtitle duration="2038" effect="" end="5040636" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1035" start="5038598" style="Default" text="I didn't dishonor Father's or Brother's name" translation=""/>
    <subtitle duration="2696" effect="" end="5043432" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1036" start="5040736" style="Default" text="It went well.&#10;It didn't just go well, it was brilliant." translation=""/>
    <subtitle duration="1214" effect="" end="5044746" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1037" start="5043532" style="Default" text="Didn't you see his garland and bouquet?" translation=""/>
    <subtitle duration="1000" effect="" end="5046110" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1038" start="5045110" style="Default" text="Where's Brother?" translation=""/>
    <subtitle duration="1432" effect="" end="5047963" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1039" start="5046531" style="Default" text="He headed out to Mannarkadu yesterday" translation=""/>
    <subtitle duration="1000" effect="" end="5049063" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1040" start="5048063" style="Default" text="saying that he had to meet someone" translation=""/>
    <subtitle duration="1814" effect="" end="5050977" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1041" start="5049163" style="Default" text="He hasn't returned&#10;Brother had come to Coimbatore" translation=""/>
    <subtitle duration="2780" effect="" end="5053857" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1042" start="5051077" style="Default" text="Really? Didn't I tell you, Ramani, that he couldn't not go?" translation=""/>
    <subtitle duration="1000" effect="" end="5054957" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1043" start="5053957" style="Default" text="He attended the concert" translation=""/>
    <subtitle duration="1000" effect="" end="5056057" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1044" start="5055057" style="Default" text="He was really happy" translation=""/>
    <subtitle duration="2105" effect="" end="5058708" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1045" start="5056603" style="Default" text="He took of this necklace of Father's and put it on me" translation=""/>
    <subtitle duration="1735" effect="" end="5060746" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1046" start="5059011" style="Default" text="I looked for him everywhere after the concert" translation=""/>
    <subtitle duration="1348" effect="" end="5062194" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1047" start="5060846" style="Default" text="I thought he'd be here by now" translation=""/>
    <subtitle duration="1651" effect="" end="5063945" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1048" start="5062294" style="Default" text="He'll be here before evening" translation=""/>
    <subtitle duration="2191" effect="" end="5066236" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1049" start="5064045" style="Default" text="What? Mother and daughter have locked up the house and are here?" translation=""/>
    <subtitle duration="1837" effect="" end="5068173" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1050" start="5066336" style="Default" text="Well, how long can we wait for someone?" translation=""/>
    <subtitle duration="1533" effect="" end="5069806" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1051" start="5068273" style="Default" text="You said you'd be back before noon" translation=""/>
    <subtitle duration="1787" effect="" end="5071693" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1052" start="5069906" style="Default" text="Never mind that, I'll drop by home and head back" translation=""/>
    <subtitle duration="1635" effect="" end="5073428" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1053" start="5071793" style="Default" text="Now, why are you going there?" translation=""/>
    <subtitle duration="1500" effect="" end="5075028" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1054" start="5073528" style="Default" text="We've brought your &lt;i&gt;mundu&lt;/i&gt;, shirt and everything else" translation=""/>
    <subtitle duration="1567" effect="" end="5076695" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1055" start="5075128" style="Default" text="When there's a hundred things to do here" translation=""/>
    <subtitle duration="1000" effect="" end="5077795" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1056" start="5076795" style="Default" text="Here, you go" translation=""/>
    <subtitle duration="1000" effect="" end="5078895" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1057" start="5077895" style="Default" text="I already have one, give it to Mother" translation=""/>
    <subtitle duration="1000" effect="" end="5079995" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1058" start="5078995" style="Default" text="Take it" translation=""/>
    <subtitle duration="1000" effect="" end="5081095" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1059" start="5080095" style="Default" text="Go ahead and take it" translation=""/>
    <subtitle duration="2271" effect="" end="5083466" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1060" start="5081195" style="Default" text="Really? All this bashfulness in your old age. Just take it" translation=""/>
    <subtitle duration="1000" effect="" end="5084566" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1061" start="5083566" style="Default" text="Take it!&#10;Oh, be still" translation=""/>
    <subtitle duration="1819" effect="" end="5089512" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1062" start="5087693" style="Default" text="We've been pushing everything, day after day" translation=""/>
    <subtitle duration="1769" effect="" end="5091381" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1063" start="5089612" style="Default" text="we're really in a tight place now" translation=""/>
    <subtitle duration="2005" effect="" end="5093486" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1064" start="5091481" style="Default" text="Father, we still have 3-4 days" translation=""/>
    <subtitle duration="3134" effect="" end="5096720" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1065" start="5093586" style="Default" text="Just because we have a list, should we buy all these vegetables and let them wither?" translation=""/>
