    <subtitle duration="15022" effect="" end="15022" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="0" start="0" style="Default" text="&lt;i&gt;Subtitles by&lt;/i&gt;&#10;&lt;b&gt;Tushtu&lt;/b&gt;" translation=""/>
    <subtitle duration="1000" effect="" end="468941" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="1" start="467941" style="Default" text="Hey, kutta" translation=""/>
    <subtitle duration="1000" effect="" end="470273" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="2" start="469273" style="Default" text="Hey, Kunjunni" translation=""/>
    <subtitle duration="1173" effect="" end="471546" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="3" start="470373" style="Default" text="Hey!" translation=""/>
    <subtitle duration="1000" effect="" end="472646" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="4" start="471646" style="Default" text="You'll fall on face" translation=""/>
    <subtitle duration="2560" effect="" end="475760" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="5" start="473200" style="Default" text="Instead of nodding off like this, why don't you clear the bed and sleep on it?" translation=""/>
    <subtitle duration="1000" effect="" end="477106" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="6" start="476106" style="Default" text="Hasn't the Sir come yet?" translation=""/>
    <subtitle duration="1000" effect="" end="478206" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="7" start="477206" style="Default" text="If he had come, wouldn't you see him?" translation=""/>
    <subtitle duration="1000" effect="" end="479442" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="8" start="478442" style="Default" text="It's past 12 ..." translation=""/>
    <subtitle duration="1557" effect="" end="481538" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="9" start="479981" style="Default" text="This is really not done." translation=""/>
    <subtitle duration="1332" effect="" end="483130" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="10" start="481798" style="Default" text="Just wait for him to get here." translation=""/>
    <subtitle duration="1280" effect="" end="484946" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="11" start="483666" style="Default" text="This has been going on for a while" translation=""/>
    <subtitle duration="1000" effect="" end="487957" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="12" start="486957" style="Default" text="I will end this practice from today onwards&#10;What, the concert?" translation=""/>
    <subtitle duration="2201" effect="" end="490258" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="13" start="488057" style="Default" text="No, the side business after the concert!" translation=""/>
    <subtitle duration="2837" effect="" end="493735" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="14" start="490898" style="Default" text="The more I let it go, the worse it becomes." translation=""/>
    <subtitle duration="1851" effect="" end="496451" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="15" start="494600" style="Default" text="There's no point in putting it so lightly" translation=""/>
    <subtitle duration="1180" effect="" end="497731" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="16" start="496551" style="Default" text="You need to be direct" translation=""/>
    <subtitle duration="1000" effect="" end="498831" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="17" start="497831" style="Default" text="I'll tell him!" translation=""/>
    <subtitle duration="1000" effect="" end="499931" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="18" start="498931" style="Default" text="What, you don't think he'd listen to me?" translation=""/>
    <subtitle duration="1626" effect="" end="502108" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="19" start="500482" style="Default" text="Regardless of his greatness as a singer" translation=""/>
    <subtitle duration="5194" effect="" end="507402" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="20" start="502208" style="Default" text="If my expression changes, he'll quake. That goes for both brothers." translation=""/>
    <subtitle duration="2993" effect="" end="511242" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="21" start="508249" style="Default" text="But if only your expression would change. Right?" translation=""/>
    <subtitle duration="1245" effect="" end="514252" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="22" start="513007" style="Default" text="You weren't asleep?" translation=""/>
    <subtitle duration="2404" effect="" end="518006" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="23" start="515602" style="Default" text="Uncle, why can't you have a serious talk with him?" translation=""/>
    <subtitle duration="1942" effect="" end="520048" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="24" start="518106" style="Default" text="Well, in that case, can't you do the same?" translation=""/>
    <subtitle duration="1574" effect="" end="521916" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="25" start="520342" style="Default" text="There are limits to what I can discuss with him." translation=""/>
    <subtitle duration="1301" effect="" end="523317" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="26" start="522016" style="Default" text="You can rebuke him" translation=""/>
    <subtitle duration="1000" effect="" end="524417" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="27" start="523417" style="Default" text="Can I do that?" translation=""/>
    <subtitle duration="2370" effect="" end="527089" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="28" start="524719" style="Default" text="I've thought about it several times. However," translation=""/>
    <subtitle duration="3585" effect="" end="530774" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="29" start="527189" style="Default" text="When I see that walk, laugh and demeanor, I cannot help but think of your father" translation=""/>
    <subtitle duration="1993" effect="" end="532867" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="30" start="530874" style="Default" text="I have not had the audacity to sit before him except during a concert" translation=""/>
    <subtitle duration="1664" effect="" end="534631" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="31" start="532967" style="Default" text="Everywhere he went, he had devotees" translation=""/>
    <subtitle duration="1267" effect="" end="535998" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="32" start="534731" style="Default" text="How many temptations" translation=""/>
    <subtitle duration="1405" effect="" end="537503" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="33" start="536098" style="Default" text="And that too, at such important people's places." translation=""/>
    <subtitle duration="1000" effect="" end="538603" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="34" start="537603" style="Default" text="And he has never failed." translation=""/>
    <subtitle duration="1523" effect="" end="540583" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="35" start="539060" style="Default" text="Music was God to him" translation=""/>
    <subtitle duration="1180" effect="" end="541863" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="36" start="540683" style="Default" text="When the son of a man like that..." translation=""/>
    <subtitle duration="1488" effect="" end="543506" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="37" start="542018" style="Default" text="God save us." translation=""/>
    <subtitle duration="3509" effect="" end="549025" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="38" start="545516" style="Default" text="Those are merely other people's beliefs." translation=""/>
    <subtitle duration="1699" effect="" end="550824" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="39" start="549125" style="Default" text="Music is from the heart" translation=""/>
    <subtitle duration="2841" effect="" end="553765" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="40" start="550924" style="Default" text="It doesn't disappear just because you had a few drinks." translation=""/>
    <subtitle duration="3200" effect="" end="557363" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="41" start="554163" style="Default" text="And if that's the case, these Western folk partake quite often and there's music." translation=""/>
    <subtitle duration="2197" effect="" end="559872" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="42" start="557675" style="Default" text="Therefore, there shouldn't be any music there!" translation=""/>
    <subtitle duration="2841" effect="" end="562813" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="43" start="559972" style="Default" text="Thus, that's not the truth." translation=""/>
    <subtitle duration="4242" effect="" end="567155" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="44" start="562913" style="Default" text="Each group has its own practices and beliefs." translation=""/>
    <subtitle duration="1000" effect="" end="568255" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="45" start="567255" style="Default" text="And that's the right way." translation=""/>
    <subtitle duration="5709" effect="" end="574369" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="46" start="568660" style="Default" text="I've seen several great artists get sloshed&#10; before a performance." translation=""/>
    <subtitle duration="1682" effect="" end="576151" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="47" start="574469" style="Default" text="You must remember one thing" translation=""/>
    <subtitle duration="3689" effect="" end="579940" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="48" start="576251" style="Default" text="We are supposed to consume alcohol; it must not consume us" translation=""/>
    <subtitle duration="2425" effect="" end="582465" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="49" start="580040" style="Default" text="It doesn't matter which artists comes to me" translation=""/>
    <subtitle duration="2461" effect="" end="585026" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="50" start="582565" style="Default" text="I will show him a great time before he leaves." translation=""/>
    <subtitle duration="2443" effect="" end="587569" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="51" start="585126" style="Default" text="Artists are the wealth of the country." translation=""/>
    <subtitle duration="1474" effect="" end="589143" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="52" start="587669" style="Default" text="We must respect them" translation=""/>
    <subtitle duration="1000" effect="" end="590243" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="53" start="589243" style="Default" text="Love them" translation=""/>
    <subtitle duration="4249" effect="" end="594592" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="54" start="590343" style="Default" text="Now, look, the committee is not going to reimburse me for all this." translation=""/>
    <subtitle duration="1000" effect="" end="595692" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="55" start="594692" style="Default" text="It'll go straight from my pocket!" translation=""/>
    <subtitle duration="1000" effect="" end="596792" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="56" start="595792" style="Default" text="Let it go!" translation=""/>
    <subtitle duration="1385" effect="" end="598277" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="57" start="596892" style="Default" text="It's for art!" translation=""/>
    <subtitle duration="2080" effect="" end="600457" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="58" start="598377" style="Default" text="The satisfaction my soul gets is enough." translation=""/>
    <subtitle duration="1000" effect="" end="601557" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="59" start="600557" style="Default" text="I'm done." translation=""/>
    <subtitle duration="1000" effect="" end="602657" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="60" start="601657" style="Default" text="I think I overdid it a bit." translation=""/>
    <subtitle duration="3374" effect="" end="606131" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="61" start="602757" style="Default" text="If three people share one bottle, it's bad luck to not finish it!" translation=""/>
    <subtitle duration="1354" effect="" end="607585" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="62" start="606231" style="Default" text="Let's finish this off." translation=""/>
    <subtitle duration="1681" effect="" end="609366" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="63" start="607685" style="Default" text="Ah no, they're all waiting for me.&#10;Let them wait!" translation=""/>
    <subtitle duration="2046" effect="" end="611512" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="64" start="609466" style="Default" text="You only need to go once we have done this properly." translation=""/>
    <subtitle duration="1000" effect="" end="612612" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="65" start="611612" style="Default" text="Ah sorry, that won't work out." translation=""/>
    <subtitle duration="2519" effect="" end="615231" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="66" start="612712" style="Default" text="It's my younger brother and uncle.&#10;Uncle?" translation=""/>
    <subtitle duration="2218" effect="" end="617549" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="67" start="615331" style="Default" text="Don't you know Pallavoor Parameshwaran Nair?" translation=""/>
    <subtitle duration="2115" effect="" end="619764" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="68" start="617649" style="Default" text="He's the king of the mrithangam (drum)." translation=""/>
    <subtitle duration="1491" effect="" end="621355" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="69" start="619864" style="Default" text="His own uncle." translation=""/>
    <subtitle duration="1336" effect="" end="622791" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="70" start="621455" style="Default" text="He's not only my uncle" translation=""/>
    <subtitle duration="2703" effect="" end="625594" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="71" start="622891" style="Default" text="He is my guru as well. He even taught the mrithangam to my father." translation=""/>
    <subtitle duration="2062" effect="" end="627756" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="72" start="625694" style="Default" text="Oho, so your father was in the same line of work as well." translation=""/>
    <subtitle duration="1595" effect="" end="629451" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="73" start="627856" style="Default" text="What country are you from!?" translation=""/>
    <subtitle duration="2980" effect="" end="632531" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="74" start="629551" style="Default" text="Haven't you heard of Payyannoor Devanarayana Namboodri?" translation=""/>
    <subtitle duration="1526" effect="" end="634157" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="75" start="632631" style="Default" text="Who is he?&#10;Well ..." translation=""/>
    <subtitle duration="2841" effect="" end="637098" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="76" start="634257" style="Default" text="To be honest, I haven't heard of him." translation=""/>
    <subtitle duration="5362" effect="" end="643187" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="77" start="637825" style="Default" text="When I'm busy with my business I occasionally dabble in the arts." translation=""/>
    <subtitle duration="4156" effect="" end="647443" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="78" start="643287" style="Default" text="Hey. A talent like that has never been seen here.&#10;Really?" translation=""/>
    <subtitle duration="2720" effect="" end="650263" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="79" start="647543" style="Default" text="Then, next year, his concert is enough for us. What do you think?" translation=""/>
    <subtitle duration="1855" effect="" end="652218" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="80" start="650363" style="Default" text="Then your dead father needs to figure it out!" translation=""/>
    <subtitle duration="2893" effect="" end="655211" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="81" start="652318" style="Default" text="He passed away around 15 years ago." translation=""/>
    <subtitle duration="2993" effect="" end="658948" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="82" start="655955" style="Default" text="What a loss. An eternal loss." translation=""/>
    <subtitle duration="2232" effect="" end="661923" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="83" start="659691" style="Default" text="If I stay here any longer, we cannot part on good terms." translation=""/>
    <subtitle duration="2599" effect="" end="664622" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="84" start="662023" style="Default" text="Huh? What ...&#10;That's how it is." translation=""/>
    <subtitle duration="1000" effect="" end="686091" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="85" start="685091" style="Default" text="You got tired of waiting, right?" translation=""/>
    <subtitle duration="1000" effect="" end="689274" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="86" start="688274" style="Default" text="Gopi..." translation=""/>
    <subtitle duration="2094" effect="" end="691679" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="87" start="689585" style="Default" text="As they say, it just happened." translation=""/>
    <subtitle duration="2460" effect="" end="694239" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="88" start="691779" style="Default" text="The committee folk asked me to come over to give me the rest of the money." translation=""/>
    <subtitle duration="2011" effect="" end="696350" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="89" start="694339" style="Default" text="We were chatting for a while and I didn't realize how time had flown" translation=""/>
    <subtitle duration="1439" effect="" end="697889" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="90" start="696450" style="Default" text="Call everyone. Let's head out." translation=""/>
    <subtitle duration="1211" effect="" end="701695" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="91" start="700484" style="Default" text="Why are you looking at me like that?" translation=""/>
    <subtitle duration="1457" effect="" end="703252" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="92" start="701795" style="Default" text="They entertained you really well, didn't they?" translation=""/>
    <subtitle duration="1000" effect="" end="704531" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="93" start="703531" style="Default" text="Entertainment?" translation=""/>
    <subtitle duration="2846" effect="" end="710341" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="94" start="707495" style="Default" text="They poured a bit of something with my juice." translation=""/>
    <subtitle duration="1000" effect="" end="711441" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="95" start="710441" style="Default" text="Just a bit." translation=""/>
    <subtitle duration="2337" effect="" end="713878" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="96" start="711541" style="Default" text="I have to travel for a few hours, right?" translation=""/>
    <subtitle duration="1000" effect="" end="714978" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="97" start="713978" style="Default" text="It relieves the fatigue." translation=""/>
    <subtitle duration="1545" effect="" end="716623" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="98" start="715078" style="Default" text="But, this is not good for us at all." translation=""/>
    <subtitle duration="1836" effect="" end="718897" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="99" start="717061" style="Default" text="Do you know how much sister-in-law worries?" translation=""/>
    <subtitle duration="1534" effect="" end="720531" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="100" start="718997" style="Default" text="Nobody in our family has this habit." translation=""/>
    <subtitle duration="1719" effect="" end="722350" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="101" start="720631" style="Default" text="Save the sermon for later." translation=""/>
    <subtitle duration="1000" effect="" end="723450" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="102" start="722450" style="Default" text="Call the others; let's go." translation=""/>
    <subtitle duration="1000" effect="" end="724550" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="103" start="723550" style="Default" text="There's no need to travel today." translation=""/>
    <subtitle duration="1000" effect="" end="725650" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="104" start="724650" style="Default" text="They've all gone to bed." translation=""/>
    <subtitle duration="2274" effect="" end="729356" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="105" start="727082" style="Default" text="Everyone knows you've started this habit." translation=""/>
    <subtitle duration="2224" effect="" end="731680" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="106" start="729456" style="Default" text="But nobody needs to know that it has reached this state" translation=""/>
    <subtitle duration="1000" effect="" end="732780" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="107" start="731780" style="Default" text="Especially Uncle." translation=""/>
    <subtitle duration="5036" effect="" end="738501" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="108" start="733465" style="Default" text="He doesn't merely treat you with the respect befitting just his relative, or daughter's husband. " translation=""/>
    <subtitle duration="1129" effect="" end="740152" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="109" start="739023" style="Default" text="Please, don't worry him." translation=""/>
    <subtitle duration="3773" effect="" end="744025" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="110" start="740252" style="Default" text="I don't drink excessively as you guys think." translation=""/>
    <subtitle duration="1000" effect="" end="745125" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="111" start="744125" style="Default" text="I'm starting to understand that." translation=""/>
    <subtitle duration="1971" effect="" end="749415" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="112" start="747444" style="Default" text="In any case, we will only leave in the morning." translation=""/>
    <subtitle duration="3115" effect="" end="766711" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="113" start="763596" style="Default" text="Aunty, you know the guy who just moved in next door?" translation=""/>
    <subtitle duration="1000" effect="" end="767811" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="114" start="766811" style="Default" text="A baldy?" translation=""/>
    <subtitle duration="1647" effect="" end="769558" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="115" start="767911" style="Default" text="He wants to know all kinds of things." translation=""/>
    <subtitle duration="1000" effect="" end="770658" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="116" start="769658" style="Default" text="Do you sing?" translation=""/>
    <subtitle duration="1000" effect="" end="771758" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="117" start="770758" style="Default" text="Do you dance?" translation=""/>
    <subtitle duration="2213" effect="" end="774071" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="118" start="771858" style="Default" text="And then, what do people here know?" translation=""/>
    <subtitle duration="1953" effect="" end="776631" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="119" start="774678" style="Default" text="I went to him with some milk and he had me there for an hour with his questions." translation=""/>
    <subtitle duration="1751" effect="" end="780016" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="120" start="778265" style="Default" text="Oh wow, that looks beautiful" translation=""/>
    <subtitle duration="1568" effect="" end="781684" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="121" start="780116" style="Default" text="You should give it to me once it's done" translation=""/>
    <subtitle duration="1000" effect="" end="782784" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="122" start="781784" style="Default" text="Appu!" translation=""/>
    <subtitle duration="1949" effect="" end="784833" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="123" start="782884" style="Default" text="Don't you have anything to read or study?" translation=""/>
    <subtitle duration="1904" effect="" end="786837" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="124" start="784933" style="Default" text="It took you an hour to deliver some milk" translation=""/>
    <subtitle duration="1921" effect="" end="788858" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="125" start="786937" style="Default" text="Check out the loitering police" translation=""/>
    <subtitle duration="3711" effect="" end="820437" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="126" start="816726" style="Default" text="Oh Radha. Have you already started with your stitching in the morning?" translation=""/>
    <subtitle duration="1000" effect="" end="821537" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="127" start="820537" style="Default" text="Go to the kitchen." translation=""/>
    <subtitle duration="2320" effect="" end="823957" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="128" start="821637" style="Default" text="Can't you see your sister-in-law toiling away alone?" translation=""/>
    <subtitle duration="3149" effect="" end="831081" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="129" start="827932" style="Default" text="Father, your hot water for the bath is ready." translation=""/>
    <subtitle duration="2309" effect="" end="833490" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="130" start="831181" style="Default" text="Devu, haven't the kids returned?&#10;No, they haven't." translation=""/>
    <subtitle duration="1617" effect="" end="835797" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="131" start="834180" style="Default" text="They said they'd be back in the night." translation=""/>
    <subtitle duration="2257" effect="" end="838458" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="132" start="836201" style="Default" text="Ramani has been up all night waiting for them." translation=""/>
    <subtitle duration="1785" effect="" end="844083" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="133" start="842298" style="Default" text="Where is that girl?" translation=""/>
    <subtitle duration="2679" effect="" end="846862" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="134" start="844183" style="Default" text="I had sent her over here.&#10;She must be next door or something." translation=""/>
    <subtitle duration="3334" effect="" end="850668" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="135" start="847334" style="Default" text="She's so lazy. All she wants to do is dress up and loll about." translation=""/>
    <subtitle duration="1803" effect="" end="852571" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="136" start="850768" style="Default" text="I told her it was OK." translation=""/>
    <subtitle duration="4684" effect="" end="857355" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="137" start="852671" style="Default" text="If she comes in contact with the heat, or if she burns herself at the fire, that's enough for her brothers." translation=""/>
    <subtitle duration="4026" effect="" end="861481" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="138" start="857455" style="Default" text="I won't hear the end of their complaints about making her work and exhaust herself." translation=""/>
    <subtitle duration="2965" effect="" end="864546" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="139" start="861581" style="Default" text="Women should be knowledgeable about these things." translation=""/>
    <subtitle duration="3873" effect="" end="868571" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="140" start="864698" style="Default" text="When she joins a new household soon, will her brothers be around to help with the kitchen work?" translation=""/>
    <subtitle duration="2846" effect="" end="872445" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="141" start="869599" style="Default" text="I think they've arrived. Can you keep an eye on this?" translation=""/>
    <subtitle duration="1505" effect="" end="878346" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="142" start="876841" style="Default" text="Here you go." translation=""/>
    <subtitle duration="1000" effect="" end="880620" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="143" start="879620" style="Default" text="Can you put this away?" translation=""/>
    <subtitle duration="1196" effect="" end="884049" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="144" start="882853" style="Default" text="Yes, sir" translation=""/>
    <subtitle duration="2679" effect="" end="886828" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="145" start="884149" style="Default" text="Did you get worried when we didn't turn up last night?" translation=""/>
    <subtitle duration="2241" effect="" end="889169" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="146" start="886928" style="Default" text="Oh no. It's not like you're all little kids." translation=""/>
    <subtitle duration="2089" effect="" end="891358" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="147" start="889269" style="Default" text="Sure, sure. You probably haven't slept all night." translation=""/>
    <subtitle duration="1433" effect="" end="892891" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="148" start="891458" style="Default" text="Don't I know her?" translation=""/>
    <subtitle duration="1887" effect="" end="894878" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="149" start="892991" style="Default" text="Father, why don't you come in for some tea?" translation=""/>
    <subtitle duration="1000" effect="" end="895978" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="150" start="894978" style="Default" text="I'll have tea at home." translation=""/>
    <subtitle duration="1899" effect="" end="897977" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="151" start="896078" style="Default" text="Who knows what's happening there now!" translation=""/>
    <subtitle duration="1000" effect="" end="899077" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="152" start="898077" style="Default" text="Let's go!" translation=""/>
    <subtitle duration="5221" effect="" end="905489" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="153" start="900268" style="Default" text="It's hard to tell when these festival concerts start or end. Can't be sure of anything!" translation=""/>
    <subtitle duration="2880" effect="" end="908824" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="154" start="905944" style="Default" text="Were you worried?&#10;Oh, not at all. Mother and grandfather worried." translation=""/>
    <subtitle duration="3537" effect="" end="914230" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="155" start="910693" style="Default" text="There's no need to talk to Mother or Ramani about what happened yesterday." translation=""/>
    <subtitle duration="1718" effect="" end="916099" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="156" start="914381" style="Default" text="If someone asks, there are reasons." translation=""/>
    <subtitle duration="1000" effect="" end="917199" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="157" start="916199" style="Default" text="Basically, you want me to lie." translation=""/>
    <subtitle duration="1988" effect="" end="919990" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="158" start="918002" style="Default" text="Unpleasant truths don't need to be stated." translation=""/>
    <subtitle duration="2156" effect="" end="922246" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="159" start="920090" style="Default" text="Unpleasant actions don't need to be committed." translation=""/>
    <subtitle duration="1398" effect="" end="924604" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="160" start="923206" style="Default" text="That, I promise." translation=""/>
    <subtitle duration="1652" effect="" end="926356" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="161" start="924704" style="Default" text="There have been several promises like this in the past." translation=""/>
    <subtitle duration="1000" effect="" end="927456" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="162" start="926456" style="Default" text="This is not like that!" translation=""/>
    <subtitle duration="1848" effect="" end="929404" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="163" start="927556" style="Default" text="This ... is for sure." translation=""/>
    <subtitle duration="3420" effect="" end="932924" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="164" start="929504" style="Default" text="What are you guys whispering about outside instead of coming in?" translation=""/>
    <subtitle duration="2678" effect="" end="936512" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="165" start="933834" style="Default" text="The things which happened yesterday. The elephant went wild!" translation=""/>
    <subtitle duration="1281" effect="" end="937893" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="166" start="936612" style="Default" text="Oh no! And then?" translation=""/>
    <subtitle duration="3234" effect="" end="941227" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="167" start="937993" style="Default" text="And then... Nothing happened, but the concert started at 2pm." translation=""/>
    <subtitle duration="2679" effect="" end="944006" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="168" start="941327" style="Default" text="What, what happened?&#10;Father, the elephant attacked!" translation=""/>
    <subtitle duration="2932" effect="" end="947038" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="169" start="944106" style="Default" text="And then?&#10;And then ... oh, the things which happened, Grandfather." translation=""/>
    <subtitle duration="1011" effect="" end="948149" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="170" start="947138" style="Default" text="Gopi, why don't you tell them?" translation=""/>
    <subtitle duration="1164" effect="" end="949413" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="171" start="948249" style="Default" text="And how am I supposed to tell them?" translation=""/>
    <subtitle duration="1449" effect="" end="950962" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="172" start="949513" style="Default" text="I need to bathe and eat something!" translation=""/>
    <subtitle duration="1096" effect="" end="952158" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="173" start="951062" style="Default" text="What's there to eat, mother?" translation=""/>
    <subtitle duration="1000" effect="" end="953258" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="174" start="952258" style="Default" text="There's dosa (savory crepe)." translation=""/>
    <subtitle duration="2050" effect="" end="955408" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="175" start="953358" style="Default" text="And was the elephant contained, Gopi?&#10;The elephant ..." translation=""/>
    <subtitle duration="2730" effect="" end="958238" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="176" start="955508" style="Default" text="Grandfather, it's time I went to the office. I'll tell you once I get back.&#10;What, what?" translation=""/>
    <subtitle duration="1195" effect="" end="960730" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="177" start="959535" style="Default" text="I wonder which elephant it was ..." translation=""/>
    <subtitle duration="4480" effect="" end="983130" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="178" start="978650" style="Default" text="Mother ... mother ... MOTHER!" translation=""/>
    <subtitle duration="1000" effect="" end="984787" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="179" start="983787" style="Default" text="What, what happened?" translation=""/>
    <subtitle duration="1000" effect="" end="985887" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="180" start="984887" style="Default" text="I called mother." translation=""/>
    <subtitle duration="1000" effect="" end="986987" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="181" start="985987" style="Default" text="To tell her that I'm leaving" translation=""/>
    <subtitle duration="1685" effect="" end="988772" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="182" start="987087" style="Default" text="Do you really need to rend your throat screaming like a demoness?" translation=""/>
    <subtitle duration="3200" effect="" end="993707" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="183" start="990507" style="Default" text="You really scared me. I thought something terrible had happened." translation=""/>
    <subtitle duration="1466" effect="" end="995273" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="184" start="993807" style="Default" text="No, it's not that. I missed Gopiettan." translation=""/>
    <subtitle duration="1719" effect="" end="997092" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="185" start="995373" style="Default" text="So what? Don't you know how to get there by yourself?" translation=""/>
    <subtitle duration="1000" effect="" end="998192" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="186" start="997192" style="Default" text="Then, get going!" translation=""/>
    <subtitle duration="3234" effect="" end="1003644" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="187" start="1000410" style="Default" text="I didn't get any sleep last night and I had just managed to fall asleep. Now, that's gone." translation=""/>
    <subtitle duration="1246" effect="" end="1006018" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="188" start="1004772" style="Default" text="I'm here for an audit." translation=""/>
    <subtitle duration="1000" effect="" end="1007214" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="189" start="1006214" style="Default" text="I'm from Piravam." translation=""/>
    <subtitle duration="1298" effect="" end="1008612" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="190" start="1007314" style="Default" text="Sir, are you here with your family?" translation=""/>
    <subtitle duration="1000" effect="" end="1009712" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="191" start="1008712" style="Default" text="No, I'm here alone." translation=""/>
    <subtitle duration="1562" effect="" end="1011374" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="192" start="1009812" style="Default" text="I have met your mother and grandfather." translation=""/>
    <subtitle duration="1786" effect="" end="1013260" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="193" start="1011474" style="Default" text="In any case, I'm quite lucky to have gotten that house." translation=""/>
    <subtitle duration="3538" effect="" end="1016898" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="194" start="1013360" style="Default" text="Because I can tell people at home that I was Kalloor Ramanathan's neighbor for a while" translation=""/>
    <subtitle duration="1517" effect="" end="1018515" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="195" start="1016998" style="Default" text="You shouldn't be this arrogant" translation=""/>
    <subtitle duration="1499" effect="" end="1020317" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="196" start="1018818" style="Default" text="Can't you turn around when someone's calling you?" translation=""/>
    <subtitle duration="2544" effect="" end="1022961" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="197" start="1020417" style="Default" text="In any case, I get the hint. Nowadays you always ignore me." translation=""/>
    <subtitle duration="1000" effect="" end="1024061" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="198" start="1023061" style="Default" text="She's my cousin." translation=""/>
    <subtitle duration="1000" effect="" end="1025161" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="199" start="1024161" style="Default" text="Daughter of my uncle." translation=""/>
    <subtitle duration="1877" effect="" end="1027138" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="200" start="1025261" style="Default" text="Which means, she's your betrothed, right?" translation=""/>
    <subtitle duration="1000" effect="" end="1028238" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="201" start="1027238" style="Default" text="That's right" translation=""/>
    <subtitle duration="1000" effect="" end="1029338" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="202" start="1028338" style="Default" text="I'll leave you to talk." translation=""/>
    <subtitle duration="1297" effect="" end="1033269" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="203" start="1031972" style="Default" text="Devi, you need to cultivate some manners." translation=""/>
    <subtitle duration="1550" effect="" end="1034919" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="204" start="1033369" style="Default" text="Is it right to throw a tantrum when I'm speaking to someone?" translation=""/>
    <subtitle duration="1000" effect="" end="1036019" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="205" start="1035019" style="Default" text="What would he think?" translation=""/>
    <subtitle duration="1000" effect="" end="1037119" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="206" start="1036119" style="Default" text="He can get lost." translation=""/>
    <subtitle duration="3116" effect="" end="1046793" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="207" start="1043677" style="Default" text="It looks really full. We can catch the next bus. Also, I want to talk to you about something." translation=""/>
    <subtitle duration="2392" effect="" end="1049285" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="208" start="1046893" style="Default" text="You can take the next bus. You can get to your college whenever you like." translation=""/>
    <subtitle duration="4582" effect="" end="1053967" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="209" start="1049385" style="Default" text="If I'm not in my seat by 10, the superintendent will mark me as absent. That's the kind of ass he is.&#10;Gopietta!" translation=""/>
    <subtitle duration="1000" effect="" end="1055067" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="210" start="1054067" style="Default" text="Gopietta!" translation=""/>
    <subtitle duration="1000" effect="" end="1060047" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="211" start="1059047" style="Default" text="Can you stop?" translation=""/>
    <subtitle duration="1903" effect="" end="1077512" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="212" start="1075609" style="Default" text="Ah, the artist has arrived!" translation=""/>
    <subtitle duration="1566" effect="" end="1083137" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="213" start="1081571" style="Default" text="Was there a concert yesterday?" translation=""/>
    <subtitle duration="1804" effect="" end="1085041" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="214" start="1083237" style="Default" text="The superintendent has asked to see you." translation=""/>
    <subtitle duration="1000" effect="" end="1086141" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="215" start="1085141" style="Default" text="For what?" translation=""/>
    <subtitle duration="1393" effect="" end="1087634" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="216" start="1086241" style="Default" text="Must be to organize a concert!" translation=""/>
    <subtitle duration="1000" effect="" end="1102085" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="217" start="1101085" style="Default" text="Sir?" translation=""/>
    <subtitle duration="1347" effect="" end="1104190" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="218" start="1102843" style="Default" text="Yes?" translation=""/>
    <subtitle duration="1567" effect="" end="1105857" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="219" start="1104290" style="Default" text="I was told to meet you?" translation=""/>
    <subtitle duration="1753" effect="" end="1107710" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="220" start="1105957" style="Default" text="It's not because I have any desire to meet you." translation=""/>
    <subtitle duration="1365" effect="" end="1109175" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="221" start="1107810" style="Default" text="What are you playing at?" translation=""/>
    <subtitle duration="1904" effect="" end="1111348" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="222" start="1109444" style="Default" text="There was a concert by my brother yesterday." translation=""/>
    <subtitle duration="2813" effect="" end="1114261" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="223" start="1111448" style="Default" text="I don't run my office based on your brother's concerts!" translation=""/>
    <subtitle duration="2309" effect="" end="1116670" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="224" start="1114361" style="Default" text="An urgent file was requested by the RDO." translation=""/>
    <subtitle duration="1180" effect="" end="1117950" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="225" start="1116770" style="Default" text="What excuse will I give?" translation=""/>
    <subtitle duration="3504" effect="" end="1121554" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="226" start="1118050" style="Default" text="You only care about concerts. Who do you think you are? Yesudas?" translation=""/>
    <subtitle duration="2342" effect="" end="1123996" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="227" start="1121654" style="Default" text="Sir, there's no need to get so worked up." translation=""/>
    <subtitle duration="1298" effect="" end="1125394" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="228" start="1124096" style="Default" text="I'll take half a day off." translation=""/>
    <subtitle duration="2241" effect="" end="1127735" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="229" start="1125494" style="Default" text="Leave? What if I don't grant the leave?" translation=""/>
    <subtitle duration="1702" effect="" end="1129537" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="230" start="1127835" style="Default" text="Sir, this isn't something that you need to grant." translation=""/>
    <subtitle duration="1000" effect="" end="1130637" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="231" start="1129637" style="Default" text="Usually, people take the leave." translation=""/>
    <subtitle duration="1899" effect="" end="1132636" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="232" start="1130737" style="Default" text="One of these days, I'll teach you a lesson.&#10;That's it right?" translation=""/>
    <subtitle duration="1000" effect="" end="1133736" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="233" start="1132736" style="Default" text="You're not going to kill me by hanging right?" translation=""/>
    <subtitle duration="2758" effect="" end="1136594" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="234" start="1133836" style="Default" text="Let me see if I can start work without you granting me leave." translation=""/>
    <subtitle duration="3100" effect="" end="1139794" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="235" start="1136694" style="Default" text="You cannot meet the RDO regarding the office without my permission!" translation=""/>
    <subtitle duration="1000" effect="" end="1140894" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="236" start="1139894" style="Default" text="Who's the RDO? God?" translation=""/>
    <subtitle duration="1000" effect="" end="1141994" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="237" start="1140994" style="Default" text="Let me see if I can meet him." translation=""/>
    <subtitle duration="1263" effect="" end="1150286" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="238" start="1149023" style="Default" text="God, huh?" translation=""/>
    <subtitle duration="1465" effect="" end="1152812" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="239" start="1151347" style="Default" text="Let him come. I'll deal with him." translation=""/>
    <subtitle duration="2196" effect="" end="1165629" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="240" start="1163433" style="Default" text="OK, OK, I'll ask him." translation=""/>
    <subtitle duration="1263" effect="" end="1171120" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="241" start="1169857" style="Default" text="It's a complaint about you." translation=""/>
    <subtitle duration="1000" effect="" end="1172355" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="242" start="1171355" style="Default" text="The superintendent." translation=""/>
    <subtitle duration="1882" effect="" end="1174337" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="243" start="1172455" style="Default" text="Why are you the office rebel?" translation=""/>
    <subtitle duration="1600" effect="" end="1176037" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="244" start="1174437" style="Default" text="Sir, I was half an hour late." translation=""/>
    <subtitle duration="2612" effect="" end="1178749" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="245" start="1176137" style="Default" text="He didn't allow me to put it down as leave either.&#10;Ah, sit." translation=""/>
    <subtitle duration="3408" effect="" end="1184860" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="246" start="1181452" style="Default" text="I heard that you said something disrespectful about me ... ?" translation=""/>
    <subtitle duration="1423" effect="" end="1186383" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="247" start="1184960" style="Default" text="Sir, do you really think so?" translation=""/>
    <subtitle duration="2253" effect="" end="1188736" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="248" start="1186483" style="Default" text="Na, I just asked for the heck of it since he brought it up." translation=""/>
    <subtitle duration="2304" effect="" end="1191140" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="249" start="1188836" style="Default" text="Isn't he aware that we knew each other before this job?" translation=""/>
    <subtitle duration="1838" effect="" end="1193078" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="250" start="1191240" style="Default" text="I haven't mentioned it to anyone, sir." translation=""/>
    <subtitle duration="2858" effect="" end="1196036" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="251" start="1193178" style="Default" text="I don't want people to think I'm taking advantage of our relationship" translation=""/>
    <subtitle duration="1000" effect="" end="1197558" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="252" start="1196558" style="Default" text="Don't you need to clock in?" translation=""/>
    <subtitle duration="1436" effect="" end="1199479" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="253" start="1198043" style="Default" text="I'll take half the day off" translation=""/>
    <subtitle duration="2218" effect="" end="1201797" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="254" start="1199579" style="Default" text="As if you should use your vacation days to satisfy your stubbornness. " translation=""/>
    <subtitle duration="1418" effect="" end="1205983" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="255" start="1204565" style="Default" text="Connect me to the superintendent" translation=""/>
    <subtitle duration="3128" effect="" end="1210187" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="256" start="1207059" style="Default" text="He's here." translation=""/>
    <subtitle duration="1419" effect="" end="1212765" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="257" start="1211346" style="Default" text="Can you send the attendance register over?" translation=""/>
    <subtitle duration="3201" effect="" end="1217609" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="258" start="1214408" style="Default" text="Man, all government offices have this kind of jealousy " translation=""/>
    <subtitle duration="2059" effect="" end="1220048" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="259" start="1217989" style="Default" text="Back in the village office" translation=""/>
    <subtitle duration="3568" effect="" end="1223716" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="260" start="1220148" style="Default" text="I distinctly remember him kicking me around because of a few plays I did" translation=""/>
    <subtitle duration="3979" effect="" end="1228421" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="261" start="1224442" style="Default" text="Once they put on the IAS crown, there's no respect for theater or music." translation=""/>
    <subtitle duration="1211" effect="" end="1230653" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="262" start="1229442" style="Default" text="I'm free till the afternoon" translation=""/>
    <subtitle duration="1487" effect="" end="1233282" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="263" start="1231795" style="Default" text="Till then, you're going to work here" translation=""/>
    <subtitle duration="2370" effect="" end="1236033" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="264" start="1233663" style="Default" text="We can have some music and arts for a bit, what say?" translation=""/>
    <subtitle duration="2872" effect="" end="1240410" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="265" start="1237538" style="Default" text="No vistors till the afternoon; I'm very busy.&#10;OK, sir." translation=""/>
    <subtitle duration="1000" effect="" end="1276532" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="266" start="1275532" style="Default" text="Please stay" translation=""/>
    <subtitle duration="2006" effect="" end="1282223" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="267" start="1280217" style="Default" text="Doing the &lt;i&gt;deepaaradhana&lt;/i&gt; is comforting now" translation=""/>
    <subtitle duration="2405" effect="" end="1288988" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="268" start="1286583" style="Default" text="What's your name?" translation=""/>
    <subtitle duration="1678" effect="" end="1294039" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="269" start="1292361" style="Default" text="Is there anything wrong in me knowing it?" translation=""/>
    <subtitle duration="1000" effect="" end="1298849" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="270" start="1297849" style="Default" text="Ah it's Vijayan" translation=""/>
    <subtitle duration="1000" effect="" end="1299949" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="271" start="1298949" style="Default" text="Come" translation=""/>
    <subtitle duration="1000" effect="" end="1301253" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="272" start="1300253" style="Default" text="Have a seat inside" translation=""/>
    <subtitle duration="3827" effect="" end="1305180" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="273" start="1301353" style="Default" text="No, can't sit down. I'm going home tomorrow since it's a second Saturday." translation=""/>
    <subtitle duration="2651" effect="" end="1307931" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="274" start="1305280" style="Default" text="So I don't need milk tomorrow and the day after tomorrow. Do tell your mother.&#10;Will do." translation=""/>
    <subtitle duration="1000" effect="" end="1309031" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="275" start="1308031" style="Default" text="I'll take your leave. OK. " translation=""/>
    <subtitle duration="1159" effect="" end="1317584" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="276" start="1316425" style="Default" text="Who's playing the veena?" translation=""/>
    <subtitle duration="1665" effect="" end="1319349" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="277" start="1317684" style="Default" text="That's my younger sister, Radha." translation=""/>
    <subtitle duration="1228" effect="" end="1322722" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="278" start="1321494" style="Default" text="Does your sister sing as well?" translation=""/>
    <subtitle duration="1903" effect="" end="1324937" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="279" start="1323034" style="Default" text="No, she is mute." translation=""/>
    <subtitle duration="3491" effect="" end="1342254" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="280" start="1338763" style="Default" text="Sorry, I didn't know.&#10;No, it's not a problem." translation=""/>
    <subtitle duration="1679" effect="" end="1346112" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="281" start="1344433" style="Default" text="I should go" translation=""/>
    <subtitle duration="1038" effect="" end="1372667" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="282" start="1371629" style="Default" text="Aha, you're sitting here quite safely" translation=""/>
    <subtitle duration="2058" effect="" end="1375365" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="283" start="1373307" style="Default" text="You took a lot of money off me in that game the day-before-yesterday" translation=""/>
    <subtitle duration="2236" effect="" end="1377701" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="284" start="1375465" style="Default" text="Call sister-in-law; we'll play a turn." translation=""/>
    <subtitle duration="2318" effect="" end="1386818" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="285" start="1384500" style="Default" text="Don't cheat OK? &#10;As if" translation=""/>
    <subtitle duration="1298" effect="" end="1389690" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="286" start="1388392" style="Default" text="Mother's game!" translation=""/>
    <subtitle duration="2111" effect="" end="1394326" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="287" start="1392215" style="Default" text="Listen, hit it fully on the line" translation=""/>
    <subtitle duration="3132" effect="" end="1402561" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="288" start="1399429" style="Default" text="Ammai, look go for the red!" translation=""/>
    <subtitle duration="3287" effect="" end="1408616" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="289" start="1405329" style="Default" text="Now she'll play by herself for half an hour. Let's kill time talking about something." translation=""/>
    <subtitle duration="1522" effect="" end="1410830" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="290" start="1409308" style="Default" text="Then, can I ask something?" translation=""/>
    <subtitle duration="2903" effect="" end="1414515" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="291" start="1411612" style="Default" text="Is the elephant story made-up or true?" translation=""/>
    <subtitle duration="2024" effect="" end="1417283" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="292" start="1415259" style="Default" text="Elephant story? Which elephant?" translation=""/>
    <subtitle duration="3079" effect="" end="1420725" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="293" start="1417646" style="Default" text="Oh, at the concert. That's not made up!" translation=""/>
    <subtitle duration="2612" effect="" end="1424064" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="294" start="1421452" style="Default" text="She broke rank. It could've been a big problem." translation=""/>
    <subtitle duration="2028" effect="" end="1426192" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="295" start="1424164" style="Default" text="Luckily it was a female elephant. She calmed down quickly." translation=""/>
    <subtitle duration="2941" effect="" end="1429652" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="296" start="1426711" style="Default" text="Play quickly!&#10;Don't break my concentration; let me play!" translation=""/>
    <subtitle duration="1315" effect="" end="1431538" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="297" start="1430223" style="Default" text="Does a female elephant go wild?" translation=""/>
    <subtitle duration="2128" effect="" end="1435223" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="298" start="1433095" style="Default" text="A female elephant doesn't go wild, does it?" translation=""/>
    <subtitle duration="3377" effect="" end="1438700" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="299" start="1435323" style="Default" text="Actually, now I'm not sure if it were male or female" translation=""/>
    <subtitle duration="2754" effect="" end="1441554" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="300" start="1438800" style="Default" text="Do you really want to put on this show for me?" translation=""/>
    <subtitle duration="2115" effect="" end="1443769" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="301" start="1441654" style="Default" text="I've known you since you were born" translation=""/>
    <subtitle duration="1678" effect="" end="1447038" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="302" start="1445360" style="Default" text="I wanted to tell you" translation=""/>
    <subtitle duration="2300" effect="" end="1451259" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="303" start="1448959" style="Default" text="That brother has started some undesirable habits" translation=""/>
    <subtitle duration="1211" effect="" end="1454581" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="304" start="1453370" style="Default" text="I'll tell you later" translation=""/>
    <subtitle duration="1280" effect="" end="1457055" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="305" start="1455775" style="Default" text="I know, Gopi" translation=""/>
    <subtitle duration="1073" effect="" end="1459304" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="306" start="1458231" style="Default" text="I've known for a while" translation=""/>
    <subtitle duration="2180" effect="" end="1463041" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="307" start="1460861" style="Default" text="In the beginning, I thought it was just medicine" translation=""/>
    <subtitle duration="1125" effect="" end="1464736" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="308" start="1463611" style="Default" text="Only later did I understand" translation=""/>
    <subtitle duration="2162" effect="" end="1468057" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="309" start="1465895" style="Default" text="That he was adding something else to the medicine" translation=""/>
    <subtitle duration="1159" effect="" end="1470531" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="310" start="1469372" style="Default" text="I'm tired of talking about it" translation=""/>
    <subtitle duration="1000" effect="" end="1472607" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="311" start="1471607" style="Default" text="Mother, it's your turn!" translation=""/>
    <subtitle duration="1000" effect="" end="1475721" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="312" start="1474721" style="Default" text="What happened, mother?" translation=""/>
    <subtitle duration="1000" effect="" end="1476821" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="313" start="1475821" style="Default" text="Nothing happened!" translation=""/>
    <subtitle duration="1637" effect="" end="1478558" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="314" start="1476921" style="Default" text="Red, where's the red coin?" translation=""/>
    <subtitle duration="1000" effect="" end="1479658" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="315" start="1478658" style="Default" text="We knocked that in a while back!" translation=""/>
    <subtitle duration="1828" effect="" end="1481586" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="316" start="1479758" style="Default" text="Then, what's the point of playing!?" translation=""/>
    <subtitle duration="1000" effect="" end="1482686" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="317" start="1481686" style="Default" text="Hey, Father is coming!" translation=""/>
    <subtitle duration="2197" effect="" end="1488990" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="318" start="1486793" style="Default" text="It seems that everyone is playing all the time here" translation=""/>
    <subtitle duration="1180" effect="" end="1490270" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="319" start="1489090" style="Default" text="Tomorrow and day after are holidays" translation=""/>
    <subtitle duration="2859" effect="" end="1493229" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="320" start="1490370" style="Default" text="Oh, so you don't need to open your books at all for the next two days!" translation=""/>
    <subtitle duration="2457" effect="" end="1496343" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="321" start="1493886" style="Default" text="Instead of wasting time playing, can't you do your vocal exercises?" translation=""/>
    <subtitle duration="1526" effect="" end="1497969" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="322" start="1496443" style="Default" text="I'll do it early in the morning tomorrow." translation=""/>
    <subtitle duration="2768" effect="" end="1501100" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="323" start="1498332" style="Default" text="I don't need to go into work tomorrow since it's a holiday tomorrow." translation=""/>
    <subtitle duration="1786" effect="" end="1502986" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="324" start="1501200" style="Default" text="I've seen so many holidays" translation=""/>
    <subtitle duration="5020" effect="" end="1508106" layer="0" margin-l="0" margin-r="0" margin-v="0" name="" note="" path="325" start="1503086" style="Default" text="Ever since he got this government job, I have never heard him sing apart from the concerts." translation=""/>
